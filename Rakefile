require 'rubygems'
require 'rake'

begin
  require 'jeweler'
  Jeweler::Tasks.new do |gem|
    gem.name = "servlet-api"

    gem.summary = %Q{A JavaGem version of servlet-api - packaged by JavaGems (http://www.javagems.org)}



    gem.description = %Q{servlet-api packaged up in a JavaGem}

    gem.homepage = nil

    gem.authors = ["pending/unknown"]

    gem.email = "autobuild@javagems.org"
    gem.version = "2.5"

  

  end
rescue LoadError
  puts "Jeweler (or a dependency) not available. Install it with: sudo gem install jeweler"
end

task :default => :build
